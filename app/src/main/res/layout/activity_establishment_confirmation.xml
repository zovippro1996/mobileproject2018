<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/LabelEstablishmentName"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Est. Name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LabelUserID" />

    <TextView
        android:id="@+id/LabelEstablishmentType"
        android:layout_width="80dp"
        android:layout_height="17dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Est. Type"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LabelEstablishmentName" />

    <TextView
        android:id="@+id/LabelFoodType"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Food Type"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LabelEstablishmentType" />

    <TextView
        android:id="@+id/LabelLocation"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="20dp"
        android:gravity="center"
        android:text="Location"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/LabelFoodType" />

    <TextView
        android:id="@+id/textview_UserID"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="52dp"
        android:gravity="center"
        android:text="USERID"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toEndOf="@+id/LabelUserID"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView_EstablishmentName"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="44dp"
        android:gravity="center"
        android:text="ESTNAME"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/LabelEstablishmentName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toEndOf="@+id/LabelEstablishmentName"
        app:layout_constraintTop_toBottomOf="@+id/LabelEstablishmentName" />

    <TextView
        android:id="@+id/textView_EstablishmentType"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="ESTTYPE"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/LabelEstablishmentType"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toEndOf="@+id/LabelEstablishmentType"
        app:layout_constraintTop_toBottomOf="@+id/LabelEstablishmentType" />

    <TextView
        android:id="@+id/textView_FoodType"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="44dp"
        android:gravity="center"
        android:text="FOODTYPE"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/LabelFoodType"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.137"
        app:layout_constraintStart_toEndOf="@+id/LabelFoodType"
        app:layout_constraintTop_toBottomOf="@+id/LabelFoodType" />

    <TextView
        android:id="@+id/textView_Location"
        android:layout_width="77dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="8dp"
        android:gravity="center"
        android:text="LOCATION"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/LabelLocation"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.135"
        app:layout_constraintStart_toEndOf="@+id/LabelLocation"
        app:layout_constraintTop_toBottomOf="@+id/LabelLocation"
        app:layout_constraintVertical_bias="0.586" />

    <Button
        android:id="@+id/button_Confirm"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="196dp"
        android:layout_marginStart="72dp"
        android:text="Confirm"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_Edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="28dp"
        android:text="Edit"
        app:layout_constraintBottom_toTopOf="@+id/button_Confirm"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.284"
        app:layout_constraintStart_toEndOf="@+id/button_Confirm"
        app:layout_constraintTop_toBottomOf="@+id/button_Confirm" />

    <TextView
        android:id="@+id/LabelUserID"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="72dp"
        android:layout_marginTop="52dp"
        android:gravity="center"
        android:text="Added By"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>